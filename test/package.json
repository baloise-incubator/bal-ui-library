{
  "name": "e2e-tests",
  "version": "13.6.2",
  "private": true,
  "description": "Cypress E2E Tests",
  "scripts": {
    "postinstall": "npm run cy:install",
    "cy:install": "cypress install",
    "cy:open": "npm run cy:copy && cypress open --browser chrome --env type=actual --config screenshotsFolder=cypress/snapshots/actual",
    "cy:start": "node ./web-server.js",
    "cy:copy": "copyfiles --flat ../testing/src/selectors/* ./cypress/support/generated",
    "cy:visual:report": "reg-cli cypress/snapshots/actual/visual cypress/snapshots/base/visual cypress/snapshots/diff/visual -R cypress/snapshots/report.html --ignoreChange --matchingThreshold 0.15"
  },
  "dependencies": {
    "@baloise/design-system-components": "13.6.2"
  },
  "devDependencies": {
    "cypress": "~12.12.0",
    "cypress-axe": "~1.4.0",
    "cypress-file-upload": "~5.0.8",
    "cypress-split": "^1.3.15",
    "cypress-visual-regression": "~3.0.0",
    "jsdom": "~21.1.0",
    "vite": "~4.3.1",
    "vue": "~3.2.45",
    "reg-cli": "0.17.7",
    "typescript": "~4.6.4",
    "express": "~4.18.2",
    "serve-index": "~1.9.1",
    "compression": "^1.7.4"
  }
}
