  "files": [
    "dist/",
    "loader/",
    "src/styles/",
    "src/components/**/*.scss"
  ],
  "scripts": {
    "start": "npm run dev",
    "update:registry": "sed -i -e \"s#nexus.balgroupit.com/repository/npm#registry.npmjs.org#g\" package-lock.json\n",
    "==================== BUILD ====================": "================================",
    "build": "npm run build:vite && npm run build:meta && npm run build:testing && stencil build",
    "build:dev": "npm run build:meta && npm run serve:build",
    "build:meta": "node .build/contributors.js && node .build/github.js",
    "build:testing": "node .build/testing.js",
    "build:docs": "stencil build --config stencil.dev.config.ts && build-storybook -o ../../storybook  --quiet",
    "build:vite": "baloiseStencilViteFix",
    "================= DEVELOPMENT =================": "================================",
    "generate": "node ./.build/stencil/generate",
    "dev": "npm run build:meta && npm run dev:build && concurrently --names \"ðŸ§©_STENCIL__,ðŸ“•_STORYBOOK\" -c \"blue,magenta\" \"npm:dev:watch\" \"npm:dev:docs\"",
    "dev:build": "stencil build --config stencil.dev.config.ts",
    "dev:watch": "stencil build --config stencil.dev.config.ts --watch",
    "dev:docs": "start-storybook -p 6006 --quiet",
    "==================== TEST =====================": "================================",
    "test": "stencil test --spec --e2e --config stencil.test.config.ts",
    "test:watch": "stencil test --spec --e2e --watchAll --config stencil.test.config.ts",
    "test:unit": "stencil test --spec --config stencil.test.config.ts",
    "test:unit:watch": "stencil test --spec --watchAll --config stencil.test.config.ts",
    "test:e2e": "stencil test --e2e --config stencil.test.config.ts",
    "test:e2e:watch": "stencil test --e2e --watchAll --config stencil.test.config.ts",
    "log": "stencil build --config stencil.log.config.ts",
    "www": "stencil build --config stencil.test.config.ts",
    "www:watch": "stencil build --dev --serve --config stencil.test.config.ts --watch"
  },
