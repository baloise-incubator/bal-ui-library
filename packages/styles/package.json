{
  "name": "@baloise/design-system-styles",
  "version": "15.2.3",
  "description": "The Baloise Design-System is an open source project for styling awesome web applications that follow the Baloise corporate styling guidelines.",
  "repository": {
    "type": "git",
    "url": "https://github.com/baloise/design-system.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://baloise-design-system.vercel.app",
  "license": "Apache-2.0",
  "scripts": {
    "build": "node scripts/build.mjs",
    "serve": "npm run serve:fonts && npm run serve:http",
    "serve:fonts": "copyfiles --flat ../fonts/lib/* assets/fonts",
    "serve:http": "http-server --cors -c-1",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "main": "scss/baloise-design-system.sass",
  "style": "css/baloise-design-system.css",
  "files": [
    "css/",
    "sass/",
    "src/"
  ],
  "dependencies": {
    "@baloise/design-system-fonts": "15.2.2",
    "@baloise/design-system-tokens": "15.2.2",
    "cheerio": "1.0.0-rc.12",
    "prompts": "2.4.2",
    "replace-in-file": "7.0.2"
  },
  "gitHead": "4157249628694f5811f322e0a547989ca9a9c6a6"
}
